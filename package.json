{
  "name": "reactreadinglist-auth-server",
  "version": "1.0.0",
  "description": "Full Stack React App with Authentication",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node server/scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.x",
    "yarn": "1.4.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-import": "^1.10.0",
    "babel-preset-airbnb": "^3.0.1",
    "chai": "^4.2.0",
    "concurrently": "^3.5.0",
    "enzyme": "^3.7.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.6.2"
  },
  "dependencies": {
    "antd": "^3.10.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.2",
    "connect-mongo": "^2.0.1",
    "dotenv": "^6.1.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "gavagai": "^0.2.2",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1",
    "newsapi": "^2.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-test-renderer": "^16.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Audiosutras/fakenewsnetwork.git"
  },
  "bugs": {
    "url": "https://github.com/Audiosutras/fakenewsnetwork/issues"
  },
  "homepage": "https://github.com/Audiosutras/fakenewsnetwork#readme",
  "directories": {
    "test": "tests"
  }
}
